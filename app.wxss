/**app.wxss**/
page{
  height: 100%;
  background-color: #F7F7F7;
  box-sizing: border-box;
}
.container{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
   justify-content: space-between;
  box-sizing: border-box;
}
.flex{
  display: flex;
}
.fx-d-row{
  flex-direction: row;
}
.fx-d-col{
  flex-direction: column;
}
.fx-jc-sa{
  justify-content: space-around;
}
.fx-jc-sb{
   justify-content:space-between;
}
.fx-wp{
  flex-wrap:wrap;
}
.f-14{
  font-size: 14px;
  color: #777777;
}
.f-12{
  font-size: 12px;
  color: #777777;
}
.f-16{
  font-size: 16px;
}
.w-10{
  width: 100%;
}
.b-s{
    box-sizing: border-box;
}
.b-d{
  border: 1px solid green;
}
.bk-c-w{
background-color: #ffffff;
}
::-webkit-scrollbar{
  height: 0;
  width: 0;
}
