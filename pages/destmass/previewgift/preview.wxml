<view class='b-s h-w-10 bk-c-w dest-mass'>
  <view class=''>
    <view class='b-s flex-c fx-jc-sb item-mass'>
      <view class='b-s item-type'>
        <image class='item-img' mode="aspectFill" src='/public/images/no-phone-group.png'></image>
        <text class=' f-16 item-type'>无手机号人群</text>
      </view>
      <text class='f-14 item-type'>{{noPhoneCount}}人</text>
    </view>

    <view class='flex-c  fx-d-col' style='margin:60rpx;'>
      <image class='preview-img' src="{{oGift.giftCover.url}}"></image>
      <view class='f-16'>{{oGift.name}}</view>
    </view>


  </view>
</view>



<view class='flex-c w-10 b-s  com-bottom-ct'>
      <view wx:if="{{oMn.n>0}}" class='f-14 flex-c note-ct'><icon class='n-icon' style='' type="info_circle" size="20" color="green"/>已排除{{oMn.m}}天内发送过{{oMn.n}}个客户</view>
      <view class='f-14 b-s t-a-c com-btn-txt' bindtap='fSendGift'>发送</view>
   </view>
