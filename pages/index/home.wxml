<!--线索例表-搜索-->
<view class='h-ct b-s' style='display:{{showData==0?"":"none"}};background-color:white;'>
  <view class='search' style='display:{{menuType?"":"none"}}'>
    <div class="search-type">
      <div style="padding-left:20rpx;height:100rpx;display:block;">
        <div bindtap='selectType' data-id='{{item.id}}' wx:for="{{selectItem}}" wx:for-item="item" wx:key="id" class="search-type-item" style="{{item.val?'color:#8F82EC;':''}}">
          <span class="search-type-top">{{item.text}}</span>
          <span class="{{item.val?'search-type-bottom':'search-type-none'}}"></span>
        </div>
      </div>
    </div>
    <div class="search-btn">
      <span class="search-btn-span">
        <icon type='search' style='vertical-align:bottom' size='20' bindtap='searchBtn'></icon>
      </span>
    </div>
  </view>

  <!--线索列表-推荐-->
  <view style='display:{{menuType&&currentSelect==0?"block":"none"}};background-color:white;'>
    <view   wx:for="{{clueList}}" wx:for-item="oitem" wx:key="id" class="list">
      <div class="list-body"   style="{{oitem.txtStyle}}"  bindtouchstart="touchS" bindtouchmove="touchM" bindtouchend="touchE"  data-index="{{index}}">
        <div class='list-img' bindtap='openItem' data-Obj='{{oitem}}' style='position:relative;'>
          <div style='margin:10rpx 15rpx 0rpx 15rpx;display:block;'>
            <image class='cst-img' src='{{oitem.customerLogo||"../../public/images/def-userimg.png"}}'></image>
          </div>
          <div style='display:{{oitem.point?"":"none"}};background-color:red;border-radius:20rpx;position:absolute;top:12rpx;right:10rpx;width:20rpx;height:20rpx;padding:0rpx;'></div>
        </div>
        <div class="list-content" bindtap='openItem' data-Obj='{{oitem}}'>
          <div style='margin-left:10rpx;display:block;'>
            <div class="list-content-top" style="line-height:45rpx;" >
              <span class='f-16 list-content-top-r' style="text-overflow:ellipsis;">{{oitem.customerName?oitem.customerName:oitem.customerWxNickname}}</span>
              <span class="list-content-status">{{oitem.statusName}}</span>
              <span style="{{oitem.source==3||oitem.source==2?'':'display:none'}}"><image  style="width:40rpx;height:40rpx;" src='../../public/images/cule-r.png'></image></span>
              <span style="{{oitem.source==1?'':'display:none'}}"><image  style="width:40rpx;height:40rpx;" src='../../public/images/clue-pyq.png'></image></span>

              <span class='f-14 list-content-top-l' style="width:15%;float:right;vertical-align:middle;">
              <span style="{{oitem.customerWechatMobile||oitem.customerPhoneNum?'':'display:none'}}"><image  style="width:40rpx;height:40rpx;vertical-align:middle;" src='../../public/images/clue-ph.png'></image></span>
              </span>
      
            </div>
            <div class='list-content-btm'>
              <span class='f-14' style="{{oitem.remark?'display:none':''}}">
                <!-- 项目： -->
                  意向项目：<span wx:for="{{oitem.productList}}" wx:for-item="pitem" wx:key="productCode">{{index==0?pitem.productName:'、'+pitem.productName}}</span>
              </span>

              <span class='f-14' style="{{oitem.remark?'':'display:none'}}">
                <!-- 项目： -->
                  备注：<span>{{oitem.remark}}</span>
              </span>
            </div>
          </div>
        </div>
        <div class='more-btn' style='background-color:#CCCCCC;'>
          <picker class='f-14' style='height:100%;' data-Obj='{{oitem}}' bindchange="bindPickerChange_rp" mode="selector" range="{{moreItem_rp}}">
            <text class='f-14' style='color:white'>更多</text>
          </picker>
        </div>
        <div class='more-btn' style='background-color:#2FAEFF;'>
          <text bindtap='bookOption'  data-Obj='{{oitem}}'  class='f-14' style='color:white'>预约</text>
        </div>
        <div class='more-btn' style='background-color:#FFA92F;'>
          <text bindtap='followOption' data-Obj='{{oitem}}'  class='f-14' style='color:white'>再跟进</text>
        </div>
      </div>
    </view>
    <view style='display:block;height:100rpx;background-color:white;margin-top:10rpx; font-size:26rpx;text-align:center;'>
    </view>
  </view>
