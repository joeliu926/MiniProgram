.h-ct {
  height: 100%;
}

.s-ct {
  height: 65px;
}

.input-o {
  margin: 0 auto;
  padding: 6rpx 10rpx;
  border-radius: 8px;
  align-items: center;
  background-color: #f0eff5;
}

.s-input {
  height: 16rpx;
  width: 560rpx;
  text-align: center;
  padding-left: 2rpx;
}

.search {
  height: 100rpx;
  border-bottom: 1px solid #eee;
}

.search-type {
  width: 70%;
  display: inline-block;
  font-size: 30rpx;
  overflow: hidden;
}

.search-type-item {
  width: 120rpx;
  height: 50rpx;
  display: inline-block;
  text-align: center;
  padding: 10rpx 10rpx 0 10rpx;
}

.search-type-top {
  display: block;
  height: 35rpx;
  font-size: 30rpx;
  padding: 20rpx 0 0 0;
  font-weight: bold;
}

.search-type-bottom {
  border-bottom: 6rpx solid #8f82ec;
  height: 10rpx;
  width: 30rpx;
  display: inline-block;
}

.search-type-none {
  height: 10rpx;
  width: 30rpx;
  display: inline-block;
}

.search-btn {
  text-align: right;
  width: 30%;
  display: inline-block;
  height: 100%;
  vertical-align: top;
}

.search-btn-span {
  padding: 22rpx 20rpx 0 0;
  display: inline-block;
}

.list {
  overflow-x: auto;
  overflow-y: hidden;
  border-bottom: 1px solid #eee;
}

.list-body {
  width: 150%;
  display: inline-block;
}

.list-img {
  display: inline-block;
  width: 12%;
  overflow: hidden;
  vertical-align: middle;
}

.list-img div {
  padding: 10rpx;
  height: 100%;
  overflow: hidden;
}

.list-content {
  display: inline-block;
  width: 55%;
  overflow: hidden;
  vertical-align: middle;
}

.list-content-top {
  display: inline-block;
  vertical-align: top;
  width: 95%;
}

.list-content-top-r {
  display: inline-block;
  vertical-align: top;
  width: 70%;
}

.list-content-top-l {
  vertical-align: top;
  display: inline-block;
  width: 30%;
  text-align: right;
}

.list-content-btm {
  display: inline-block;
  width: 90%;
}

.list-none {
  padding: 100rpx;
  font-size: 26rpx;
  height: 200rpx;
  display: block;
}

.list-none-div {
  display: block;
  text-align: center;
  height: 80rpx;
}

.more-btn {
  display: inline-block;
  text-align: center;
  line-height: 140rpx;
  height: 140rpx;
  width: 11%;
  overflow: hidden;
  vertical-align: middle;
}

.opensh {
  border-bottom: 1px solid #eee;
  height: 60rpx;
  display: block;
  padding: 20rpx;
}

.opensh-input {
  display: inline-block;
  width: 85%;
}

.opensh-btn {
  display: inline-block;
  width: 15%;
  color: #63da65;
  text-align: center;
  font-size: 30rpx;
  vertical-align: top;
  line-height: 60rpx;
}

.add-but {
  position: fixed;
  left: 42%;
  bottom: 250rpx;
  border-radius: 100rpx;
  height: 240rpx;
  width: 280rpx;
  line-height: 240rpx;
  font-size: 22rpx;
}

.add-img {
  width: 120rpx;
  height: 120rpx;
}

.submit-btn {
  padding: 10rpx 0rpx;
  margin-bottom: 24rpx;
  display: block;
  text-align: center;
}

.submit-btn>div {
  margin: 0 auto;
  display: block;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
  background: #9083ed;
  color: #fff;
  font-size: 40rpx;
  width: 90%;
  text-align: center;
  box-sizing: border-box;
}

.contect-line {
  border-bottom: 1px solid #eee;
  margin: 0 30rpx;
  padding: 30rpx 0;
}

.close-content {
  padding: 10rpx 10rpx;
  margin-bottom: 24rpx;
  display: block;
  text-align: center;
}

.close-area {
  text-align: left;
  height: 300rpx;
  width: 95%;
  margin: 0 auto;
}

.remark {
  padding: 10rpx 10rpx;
  margin-bottom: 24rpx;
  display: block;
  text-align: center;
}

.remark-area {
  text-align: left;
  height: 300rpx;
  width: 95%;
  margin: 0 auto;
}

.menu {
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  width: 100%;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  background-color: white;
  height: 98rpx;
  border-top: 1px solid #eee;
}

.menu span {
  font-size: 22rpx;
  display: block;
  height: 30rpx;
}

.menu-l {
  display: inline-block;
  width: 42%;
  text-align: center;
}

.menu-l>image {
  width: 50rpx;
  height: 40rpx;
}

.menu-m {
  display: inline-block;
  width: 16%;
  text-align: center;
}

.menu-m>image {
  width: 110rpx;
  height: 110rpx;
  display: block;
  margin: 0 5rpx 0 5rpx;
  position: absolute;
  bottom: 30rpx;
}

.menu-r {
  display: inline-block;
  width: 42%;
  text-align: center;
}

.menu-r>image {
  width: 50rpx;
  height: 50rpx;
  margin: 0 auto;
}

.n-cst {
  padding: 0rpx 12rpx;
  height: 65rpx;
  align-items: center;
  margin: 2rpx 0 10rpx;
}

.cst-img {
  height: 96rpx;
  width: 96rpx;
}

.txt-cst {
  margin-left: 10rpx;
  color: #333;
}

.ex-ct {
  padding: 8rpx 12rpx 1rpx 12rpx;
  margin-top: 2rpx;
}

.ex-item {
  margin-left: 10px;
}

.istatus {
  background-color: #fd937a;
  border-radius: 10px;
  padding: 0px 10px;
  color: #fff;
}

.d-img {
  height: 69px;
  width: 69px;
  position: absolute;
  z-index: 100;
  bottom: 20px;
  left: 40%;
}
