.h-ct {
  height: 100%;
}

.s-ct {
  height: 65px;
}

.input-o {
  margin: 0 auto;
  padding: 6rpx 10rpx;
  border-radius: 8px;
  align-items: center;
    height: 60rpx;
  background-color: #f0eff5;
}

.s-input {
  height: 55rpx;
  width: 560rpx;
  text-align: center;
  padding-left: 2rpx;
  font-size: 30rpx;
}

.search {
  height: 100rpx;
  border-bottom: 1rpx solid #eee;
}

.search-type {
  width: 70%;
  display: inline-block;
  overflow: hidden;
  height: 100rpx;
}

.search-type-item {
  vertical-align: top;
  height: 100rpx;
  padding: 0 20rpx 0 0;
  display: inline-block;
  text-align: center;
  position: relative;
}

.search-type-top {
  display: block;
  height: 35rpx;
  font-size: 32rpx;
  padding: 20rpx 0 0 0;
  font-weight: bold;
}

.search-type-bottom {
  border-bottom: 6rpx solid #8f82ec;
  height: 10rpx;
  width: 30rpx;
  display: inline-block;
  position: absolute;
  bottom: 0rpx;
  left:28%;
}

.search-type-none {
  height: 10rpx;
  width: 30rpx;
  display: inline-block;
}

.search-btn {
  text-align: right;
  width: 30%;
  display: inline-block;
  height: 100%;
  vertical-align: top;
}

.search-btn-span {
  padding: 17rpx 30rpx 0 0;
  display: inline-block;
}

.list {
  overflow-x: hidden;
  overflow-y: hidden;
  border-bottom: 1rpx solid #eee;
  position: relative;
  height: 140rpx;
}

.list-body {
  width: 150%;
  display: inline-block;
  position: absolute;
}

.list-img {
  display: inline-block;
  width: 12%;
  overflow: hidden;
  vertical-align: middle;
}

.list-img div {
  padding: 10rpx;
  height: 100%;
  overflow: hidden;
}

.list-content {
  display: inline-block;
  width: 55%;
  overflow: hidden;
  vertical-align: middle;
}

.list-content-top {
  display: inline-block;
  vertical-align: top;
  width: 96%;
}

.list-content-top-r {
  display: inline-block;
  vertical-align: top;
  height: 50rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}

.list-content-status{
  font-size:22rpx;border:1rpx solid #4BB9FF;border-radius:22rpx;color:#4BB9FF;vertical-align:top;margin-right:10rpx;padding:2rpx 10rpx;margin-left: 10rpx;
}

.list-content-top-l {
  vertical-align: top;
  display: inline-block;
  width: 35%;
    text-overflow:ellipsis;
  white-space:nowrap;
  text-align: right;
}

.list-content-btm {
  height: 50rpx;
  vertical-align: top;
  text-overflow: ellipsis;
      white-space:nowrap;
  display: inline-block;

    overflow: hidden;
  width: 90%;
}

.list-none {
  padding: 100rpx;
  font-size: 26rpx;
  height: 200rpx;
  display: block;
}

.list-none-div {
  display: block;
  text-align: center;
  height: 80rpx;
}

.more-btn {
  display: inline-block;
  text-align: center;
  line-height: 140rpx;
  height: 140rpx;
  width: 11%;
  overflow: hidden;
  vertical-align: middle;
}

.opensh {
  border-bottom: 1rpx solid #eee;
  height: 70rpx;
  display: block;
  padding: 20rpx;
}

.opensh-input {
  display: inline-block;
  width: 85%;
}

.opensh-btn {
  display: inline-block;
  width: 15%;
  color: #63da65;
  text-align: center;
  font-size: 30rpx;
  vertical-align: top;
  line-height: 70rpx;
}

.add-but {
  line-height: 240rpx;
  font-size: 22rpx;
   position: fixed;
   top:0;
   bottom: 0;  
   left: 0;
   right: 0;
   background: rgba(0, 0, 0, 0.5);
}

.add-img {
  width: 120rpx;
  height: 120rpx;
}

.submit-btn {
  padding: 10rpx 0rpx;
  margin-bottom: 24rpx;
  display: block;
  text-align: center;
}

.submit-btn>div {
  margin: 0 auto;
  display: block;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  background: #9083ed;
  color: #fff;
  font-size: 40rpx;
  width: 450rpx;
  text-align: center;
  box-sizing: border-box;
}

.contect-line {
  border-bottom: 1rpx solid #eee;
  margin: 0 40rpx;
  padding: 30rpx 0;
}

.close-content {
  padding: 10rpx 10rpx;
  margin-bottom: 24rpx;
  display: block;
  text-align: center;
}

.close-area {
  text-align: left;
  height: 300rpx;
  width: 95%;
  margin: 0 auto;
}

.remark {
  padding: 10rpx 10rpx;
  margin-bottom: 24rpx;
  display: block;
  text-align: center;
}

.remark-area {
  text-align: left;
  height: 300rpx;
  width: 95%;
  margin: 0 auto;
}

.menu {
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  width: 100%;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  background-color: white;
  height: 98rpx;
  border-top: 1rpx solid #eee;
}

.menu span {
  font-size: 22rpx;
  display: block;
  height: 30rpx;
}

.menu-l {
  display: inline-block;
  width: 42%;
  text-align: center;
}

.menu-l>image {
  width: 50rpx;
