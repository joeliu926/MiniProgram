<!--logs.wxml-->
<view class="container log-list">
  <block wx:for="{{logs}}" wx:for-item="log" wx:key="id">
    <text class="log-item">{{index + 1}}. {{log}}</text>
  </block>
<view>
<button open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber">获取手机号码</button>
</view>
<view style='width:100%;'>
<map id="map" longitude="{{longitude}}" latitude="{{latitude}}" scale="14" 
bindregionchange="regionchange" show-location style="width:100%; height:500rpx;"></map>
</view>



</view>
