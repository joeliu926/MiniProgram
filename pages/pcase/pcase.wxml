<view class='case-content'>
<block wx:for="{{projectItems}}" wx:for-item="oitem">
  <view class='o-flex case-item-content'>
    <text class='case-title'  bindtap='selectTitle'>{{oitem.productName}}</text>
    <view class='case-item-detail'>
       <block wx:for="{{oitem.productList}}" wx:for-item="secitem">
           <block wx:for="{{secitem.productList}}" wx:for-item="item">
             <text class='case-item' data-paid='{{item.parentCode}}' data-paname='{{oitem.productName}}' data-itemid='{{item.productCode}}' data-iname='{{item.productName}}' bindtap='selectItem'>{{item.productName}}</text>
          </block>
       </block>
    </view>
  </view>
</block>
<view class='b-pos' style=''> </view>
</view>

<view class='case-btn-content'>
  <view class='o-flex'>
      <image class='case-img-btn' src='../../public/images/case.png' mode='aspectFill'></image>
      <text class='case-btn-name'>案例</text>
  </view>
</view>

