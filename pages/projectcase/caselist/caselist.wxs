var arrayFilter=function(oItems,item){
  oItems = oItems||[];
  return oItems.indexOf(item)>=0;
}

var inArray = function (arr, item) {
  // console.log(arr.indexOf);
  if (arr.indexOf(item) == -1) {
    return false;
  } else {
    return true;
  }
}
module.exports = {
  arrayFilter: arrayFilter,
   inArray: inArray
}
